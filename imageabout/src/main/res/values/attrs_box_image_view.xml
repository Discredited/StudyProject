<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BoxImageView">
        <attr name="BoxImageRadius" format="dimension" />
        <attr name="BoxImageOverTextSize" format="dimension" />
        <attr name="BoxImageCornerType" format="integer">
            <enum name="all" value="0" />
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4"/>
            <enum name="diagonalLeft" value="5" />
            <enum name="diagonalRight" value="6" />
            <enum name="leftTop" value="7" />
            <enum name="rightTop" value="8" />
            <enum name="leftBottom" value="9" />
            <enum name="rightBottom" value="10" />
        </attr>
    </declare-styleable>
</resources>